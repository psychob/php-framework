{
    "name": "psychob/framework",
    "description": "a simple php framework",
    "type": "library",
    "require": {
        "php": "^7.2",
        "psr/container": "^1.0",
        "symfony/finder": "^4.3"
    },
    "require-dev": {
        "mikey179/vfsstream": "^1.6",
        "mockery/mockery": "^1.2",
        "phpunit/phpunit": "^8.2",
        "symfony/var-dumper": "^4.3"
    },
    "license": "MPL-2.0",
    "authors": [
        {
            "name": "Andrzej Budzanowski",
            "email": "kontakt@andrzej.budzanowski.pl"
        }
    ],
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "PsychoB\\Framework\\": "./src"
        },
        "files": [
            "./src/Framework/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\PsychoB\\Framework\\": "./tests"
        }
    },
    "config": {
        "sort-packages": true
    }
}
