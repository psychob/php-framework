{
    "name": "psychob/framework",
    "description": "a simple php framework",
    "type": "library",
    "require": {
        "php": "^7.2",
        "psychob/dependency-injection": "^0.7.1",
        "symfony/var-dumper": "^4.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.2",
        "mockery/mockery": "^1.2"
    },
    "license": "MPL-2.0",
    "authors": [
        {
            "name": "Andrzej Budzanowski",
            "email": "kontakt@andrzej.budzanowski.pl"
        }
    ],
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "PsychoB\\Framework\\": "./src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\PsychoB\\Framework\\": "./tests"
        }
    },
    "config": {
        "sort-packages": true
    }
}
